{
  "name": "table",
  "description": "Table module for jviz",
  "version": "0.0.1",
  "author": { "id": "jmjuanes", "name": "Jose Miguel Juanes", "email": "josemijuanes@gmail.com" },
  "directory": "./build",
  "repository": "https://github.com/jviz/module-table",
  "dependencies": { "jviz": "dev" },
  "tasks":
  {
    "build:js":
    [
      { "name": "src", "args": [ "./js/table.js", "./js/**.js" ] },
      { "name": "concat", "args": "table.js" },
      { "name": "dest", "args": "./" }
    ],
    "build:scss":
    [
      { "name": "src", "args": "./scss/**.scss" },
      { "name": "sass" },
      { "name": "dest", "args": "./" }
    ],
    "build:img":
    [
      { "name": "src", "args": "./img/**.svg" },
      { "name": "dest", "args": "./img/" }
    ]
  },
  "build": [ "build:js", "build:scss", "build:img" ],
  "install": [ ]
}
